<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!--#region general-->

    <sys:String x:Key="ok">OK</sys:String>
    <sys:String x:Key="cancel">Annuler</sys:String>

    <sys:String x:Key="error.saving.caption">Erreur lors de l'enregistrement</sys:String>
    <sys:String x:Key="error.saving.message">La ressource n'a pas pu être enregistrée</sys:String>

    <sys:String x:Key="error.loading.caption">Erreur lors du chargement</sys:String>
    <sys:String x:Key="error.loading.message">La ressource n'a pas pu être chargée</sys:String>

    <!--#endregion general-->

    <!--#region download path-->

    <sys:String x:Key="file.invalidCharacters.caption">Nom de fichier local</sys:String>
    <sys:String x:Key="file.invalidCharacters.message">Le chemin local contient des caractères non valides</sys:String>

    <sys:String x:Key="file.pathToLong.caption">Chemin de fichier local</sys:String>
    <sys:String x:Key="file.pathToLong.message">Le chemin local est trop long</sys:String>

    <!--#endregion download path-->

    <!--#region windows-->

    <!--#region log in window-->

    <sys:String x:Key="logIn.username">Nom d'utilisateur :</sys:String>
    <sys:String x:Key="logIn.password">Mot de passe :</sys:String>
    <sys:String x:Key="logIn.rememberMe">Se souvenir de moi</sys:String>
    <sys:String x:Key="logIn.logIn">Connexion</sys:String>

    <sys:String x:Key="logIn.error.caption">Erreur de lecture des paramètres</sys:String>
    <sys:String x:Key="logIn.error.message">Nom d'utilisateur et mot de passe n'ont pas pu être lus</sys:String>

    <!--#endregion log in window-->

    <!--#region settings window-->

    <sys:String x:Key="settings.title">Paramètres</sys:String>
    <sys:String x:Key="settings.language">Langue :</sys:String>

    <sys:String x:Key="settings.env.production">Production</sys:String>
    <sys:String x:Key="settings.env.beta">Beta</sys:String>

    <!--#endregion settings window-->

    <!--#endregion windows-->

    <!--#region API-->

    <!--#region API Key-->

    <sys:String x:Key="api.APIExpired.caption">Clé API</sys:String>
    <sys:String x:Key="api.APIExpired.message">La clé API a expiré</sys:String>

    <!--#endregion API Key-->

    <!--#region titles-->

    <!--#region authentication-->

    <sys:String x:Key="api.authentication.postApiLogIn">Erreur lors de la récupération de la clé API</sys:String>

    <!--#endregion authentication-->

    <!--#region project-->

    <sys:String x:Key="api.project.post">Erreur lors de la création du projet</sys:String>
    <sys:String x:Key="api.project.get">Erreur lors de la récupération des projets</sys:String>
    <sys:String x:Key="api.project.getMetadata">Erreur lors de la récupération des métadonnées du projet</sys:String>
    <sys:String x:Key="api.project.getFolders">Erreur lors de la récupération des dossiers</sys:String>

    <!--#endregion project-->

    <!--#region folder-->

    <sys:String x:Key="api.folder.get">Erreur lors de la récupération des dossiers et/ou documents</sys:String>
    <sys:String x:Key="api.folder.getMetadata">Erreur lors de la récupération des métadonnées du dossier</sys:String>
    <sys:String x:Key="api.folder.post">Erreur lors de la création des dossiers</sys:String>
    <sys:String x:Key="api.folder.getDocuments">Erreur lors de la récupération des documents</sys:String>
    <sys:String x:Key="api.folder.getFolders">Erreur lors de la récupération des dossiers</sys:String>
    <sys:String x:Key="api.folder.getImages">Erreur lors de la récupération des images</sys:String>
    <sys:String x:Key="api.folder.getProperties">Erreur lors de la récupération des propriétés du dossier</sys:String>

    <!--#endregion folder-->

    <!--#region equipment-->

    <sys:String x:Key="api.equipment.get">Erreur lors de la récupération de l'équipement</sys:String>

    <!--#endregion equipment-->

    <!--#region document-->

    <sys:String x:Key="api.document.getUploudURL">Erreur lors de la récupération de l'URL de téléversement du fichier</sys:String>
    <sys:String x:Key="api.document.upload">Erreur lors du téléversement du fichier</sys:String>
    <sys:String x:Key="api.document.getMetadata">Erreur lors de la récupération des métadonnées du document</sys:String>
    <sys:String x:Key="api.document.delete">Erreur lors de la suppression du document</sys:String>
    <sys:String x:Key="api.document.getDownloadURL">Erreur lors de la récupération de l'URL de téléchargement du document</sys:String>
    <sys:String x:Key="api.document.download">Erreur lors du téléchargement du fichier</sys:String>

    <!--#endregion document-->

    <!--#region logpoint-->

    <sys:String x:Key="api.logpoint.get">Erreur lors de la récupération des points de journalisation</sys:String>

    <!--#endregion logpoint-->

    <!--#region property-->

    <sys:String x:Key="api.property.getProject">Erreur lors de la récupération des propriétés du projet</sys:String>
    <sys:String x:Key="api.property.addProject">Erreur lors de l'ajout des propriétés au projet</sys:String>

    <!--#endregion property-->

    <!--#region undocumented endpoints-->

    <sys:String x:Key="api.deleteFileFolder">Erreur lors de la suppression du dossier</sys:String>

    <!--#endregion undocumented endpoints-->

    <!--#endregion titles-->

    <!--#region messages-->

    <sys:String x:Key="api.default">Une erreur inattendue s'est produite</sys:String>

    <!--#region status codes-->

    <sys:String x:Key="api.400.1">Les paramètres fournis sont insuffisants ou invalides</sys:String>
    <sys:String x:Key="api.400.2">La requête était invalide ou contenait des paramètres mal formés</sys:String>

    <sys:String x:Key="api.401">L'authentification a échoué</sys:String>

    <sys:String x:Key="api.403.1">Accès refusé au projet cible pour l'utilisateur de la session</sys:String>
    <sys:String x:Key="api.403.2">Échec de l'authentification (identifiants invalides)</sys:String>
    <sys:String x:Key="api.403.3">L'utilisateur a été authentifié, mais il manque des privilèges</sys:String>
    <sys:String x:Key="api.403.4">Aucune autorisation pour créer des objets</sys:String>

    <sys:String x:Key="api.404.1">Le projet n'existe pas</sys:String>
    <sys:String x:Key="api.404.2">Le dossier n'existe pas</sys:String>
    <sys:String x:Key="api.404.3">Le document n'existe pas</sys:String>
    <sys:String x:Key="api.404.4">Affectation non trouvée dans la base de données</sys:String>

    <sys:String x:Key="api.409">Le document identique existe déjà</sys:String>

    <sys:String x:Key="api.500">Échec de la ressource interne d'Infrakit</sys:String>

    <!--#endregion status codes-->

    <sys:String x:Key="api.timeout">Une erreur de délai d'attente s'est produite</sys:String>

    <sys:String x:Key="api.loadProjects">Les projets n'ont pas pu être chargés</sys:String>

    <!--#endregion messages-->

    <!--#endregion API-->

</ResourceDictionary>